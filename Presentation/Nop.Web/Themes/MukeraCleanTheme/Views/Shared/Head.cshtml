@using Nop.Core
@using Nop.Services.Helpers
@using Nop.Web.Framework.UI
@inject IWorkContext workContext
@inject IThemeContext themeContext
@inject IUserAgentHelper userAgentHelper
@{
    var supportRtl = workContext.WorkingLanguage.Rtl;
    var themeName = themeContext.WorkingThemeName;     

    //add jQuery UI css file  
    Html.AppendCssFileParts($"~/Themes/{themeName}/Content/css/addu_custom_theme_styles_sheet.css"); 
    Html.AppendCssFileParts($"~/Themes/{themeName}/Content/css/one_new_theme_css.css");   
    
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
   <!-- Html.AppendCssFileParts($"~/Themes/{themeName}/Content/css/bootstrap.min.css"); -->
    <!-- Latest compiled and minified CSS -->

    Html.AppendCssFileParts($"~/Themes/{themeName}/Content/assets/css/style.css"); 
    Html.AppendCssFileParts($"~/Themes/{themeName}/Content/assets/css/vendor.css");                     
    

    //add main CSS file
    if (supportRtl)
    {
        Html.AppendCssFileParts($"~/Themes/{themeName}/Content/css/styles.rtl.css"); 
    }
    else
    {
        Html.AppendCssFileParts($"~/Themes/{themeName}/Content/css/styles.css"); 
    }

    Html.AppendCssFileParts("~/lib/jquery-ui/jquery-ui-1.12.1.custom/jquery-ui.min.css"); 
}