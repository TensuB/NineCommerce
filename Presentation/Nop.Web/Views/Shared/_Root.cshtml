                <li><a href="" class="icon-linkedin text-linkeding fs-16"></a></li>
                <li><a href="" class="icon-pinterest-square text-pinterest fs-16"></a></li>                            
            </ul>
        </div>
        </div>
         <div class="row justify-content-between">
            <div class="col-md-5"></div> 
            <div class="col-md-2 header-accent-hr-container" data-aos="fade-up">                   
                <hr class="header-accent-hr" /> 
            </div> 
            <div class="col-md-5"></div> 
             <div class="col-md-12 text-center">&copy; Copy Right Reserved - @DateTime.Today.Year.ToString() - Dr Barry J Toole</div>             
         </div>   
        </div>        
    </div>
</section>
</footer>    
<!-- / footer -->

