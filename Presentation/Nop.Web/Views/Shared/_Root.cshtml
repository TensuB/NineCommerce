                <li class="nav-item"><a class="nav-link text-gray-dark" href="@Url.RouteUrl("CustomerFaqs")">FAQs</a></li>       
            </ul>
        </div>
        <div class="col-sm-12 col-md-6 col-lg-4 text-center text-md-left" data-aos="fade-left">
            <h3 style="opacity: 0.86;">Connect with Us</h3>
